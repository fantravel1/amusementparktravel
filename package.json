{
  "name": "amusementparktravel",
  "version": "1.0.0",
  "description": "The world's most comprehensive amusement park travel planning resource",
  "scripts": {
    "dev": "npx @11ty/eleventy --serve --watch",
    "build": "npx @11ty/eleventy && npm run build:css",
    "build:css": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./_site/assets/css/styles.css --minify",
    "preview": "npx serve _site"
  },
  "keywords": [
    "theme-parks",
    "travel",
    "amusement-parks",
    "universal",
    "disney",
    "vacation-planning"
  ],
  "author": "AmusementParkTravel.com",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "alpinejs": "^3.13.3"
  }
}
