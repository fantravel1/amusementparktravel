<script>
function expressPassCalculator() {
  return {
    selectedPark: 'universal-orlando',
    crowdLevel: 'moderate',
    partySize: 2,
    hoursInPark: 10,
    targetRides: 10,
    passType: 'express',
    hourlyValue: 25,

    parkData: {
      passOptions: [
        { id: 'express', name: 'Universal Express', price: 90 },
        { id: 'express-unlimited', name: 'Express Unlimited', price: 170 }
      ],
      avgWaitMultipliers: { low: 0.5, moderate: 1.0, high: 1.5, extreme: 2.0 },
      baseAvgWait: 45,
      expressWaitReduction: 0.85
    },

    parkConfigs: {
      'universal-orlando': {
        passOptions: [
          { id: 'express', name: 'Universal Express', price: 90 },
          { id: 'express-unlimited', name: 'Express Unlimited', price: 170 }
        ],
        baseAvgWait: 45,
        expressWaitReduction: 0.85
      },
      'universal-epic': {
        passOptions: [
          { id: 'express', name: 'Express Pass', price: 140 },
          { id: 'express-unlimited', name: 'Express Unlimited', price: 260 }
        ],
        baseAvgWait: 60,
        expressWaitReduction: 0.85
      },
      'disney-world': {
        passOptions: [
          { id: 'lightning-single', name: 'Lightning Lane Single', price: 15 },
          { id: 'lightning-multi', name: 'Lightning Lane Multi (est. 3 rides)', price: 85 }
        ],
        baseAvgWait: 50,
        expressWaitReduction: 0.90
      },
      'disneyland': {
        passOptions: [
          { id: 'lightning-single', name: 'Lightning Lane Single', price: 20 },
          { id: 'lightning-multi', name: 'Lightning Lane Multi (est. 3 rides)', price: 70 }
        ],
        baseAvgWait: 55,
        expressWaitReduction: 0.90
      },
      'six-flags': {
        passOptions: [
          { id: 'flash', name: 'Flash Pass', price: 55 },
          { id: 'flash-gold', name: 'Flash Pass Gold', price: 110 },
          { id: 'flash-platinum', name: 'Flash Pass Platinum', price: 180 }
        ],
        baseAvgWait: 35,
        expressWaitReduction: 0.80
      },
      'cedar-fair': {
        passOptions: [
          { id: 'fast-lane', name: 'Fast Lane', price: 80 },
          { id: 'fast-lane-plus', name: 'Fast Lane Plus', price: 130 }
        ],
        baseAvgWait: 40,
        expressWaitReduction: 0.82
      },
      'europa-park': {
        passOptions: [
          { id: 'none', name: 'No Express Option (Strategy Tips)', price: 0 }
        ],
        baseAvgWait: 35,
        expressWaitReduction: 0
      },
      'phantasialand': {
        passOptions: [
          { id: 'none', name: 'No Express Option (Strategy Tips)', price: 0 }
        ],
        baseAvgWait: 30,
        expressWaitReduction: 0
      }
    },

    init() {
      this.updateParkData();
    },

    updateParkData() {
      this.parkData = this.parkConfigs[this.selectedPark];
      this.passType = this.parkData.passOptions[0].id;
    },

    get selectedPassOption() {
      return this.parkData.passOptions.find(p => p.id === this.passType) || this.parkData.passOptions[0];
    },

    get crowdMultiplier() {
      const multipliers = { low: 0.5, moderate: 1.0, high: 1.5, extreme: 2.0 };
      return multipliers[this.crowdLevel];
    },

    get avgWaitWithout() {
      return Math.round(this.parkData.baseAvgWait * this.crowdMultiplier);
    },

    get avgWaitWith() {
      if (this.parkData.expressWaitReduction === 0) return this.avgWaitWithout;
      return Math.round(this.avgWaitWithout * (1 - this.parkData.expressWaitReduction));
    },

    get estimatedWaitWithout() {
      return this.targetRides * this.avgWaitWithout;
    },

    get estimatedWaitWith() {
      return this.targetRides * this.avgWaitWith;
    },

    get timeSaved() {
      return Math.max(0, this.estimatedWaitWithout - this.estimatedWaitWith);
    },

    get totalCost() {
      return this.selectedPassOption.price * this.partySize;
    },

    get valueOfTimeSaved() {
      return (this.timeSaved / 60) * this.hourlyValue * this.partySize;
    },

    get costPerHourSaved() {
      if (this.timeSaved === 0) return Infinity;
      return this.totalCost / (this.timeSaved / 60);
    },

    get rideTime() {
      return 5; // Average ride duration in minutes
    },

    get ridesWithout() {
      const totalMinutes = this.hoursInPark * 60;
      const timeForRides = totalMinutes * 0.7; // 70% of time for rides
      return Math.floor(timeForRides / (this.avgWaitWithout + this.rideTime));
    },

    get ridesWith() {
      const totalMinutes = this.hoursInPark * 60;
      const timeForRides = totalMinutes * 0.7;
      return Math.floor(timeForRides / (this.avgWaitWith + this.rideTime));
    },

    get extraRides() {
      return Math.max(0, this.ridesWith - this.ridesWithout);
    },

    get freeTimeWithout() {
      return Math.max(0, (this.hoursInPark * 60) - this.estimatedWaitWithout - (this.targetRides * this.rideTime));
    },

    get freeTimeWith() {
      return Math.max(0, (this.hoursInPark * 60) - this.estimatedWaitWith - (this.targetRides * this.rideTime));
    },

    get roiScore() {
      if (this.totalCost === 0) return 0;
      return this.valueOfTimeSaved / this.totalCost;
    },

    get recommendation() {
      if (this.parkData.expressWaitReduction === 0) {
        return {
          title: "Express Pass Not Available",
          message: "This park doesn't offer a paid express pass system. Use our strategy tips instead!",
          tips: [
            "Arrive at rope drop for shortest waits",
            "Use single rider lines where available",
            "Stay until closing for reduced crowds",
            "Weekdays have significantly shorter waits"
          ]
        };
      }

      if (this.roiScore >= 1.2) {
        return {
          title: "Highly Recommended! Great Value",
          message: `At ${this.crowdLevel} crowds, Express Pass will save you ${this.formatTime(this.timeSaved)} and let you experience ${this.extraRides} more rides. The time savings alone exceed the cost.`,
          tips: [
            "Book Express Pass in advance for best prices",
            "Still arrive early to maximize your day",
            "Use express on longest-wait attractions first",
            "Some attractions may not be included"
          ]
        };
      } else if (this.roiScore >= 0.7) {
        return {
          title: "Consider It - Moderate Value",
          message: "Express Pass offers decent value for your situation. Worth it if you prioritize comfort and extra experiences over strict cost efficiency.",
          tips: [
            "Compare with rope drop strategy (free)",
            "Consider splitting: buy for peak hours only",
            "Most valuable for families with young kids",
            "Check if your hotel includes express benefits"
          ]
        };
      } else {
        return {
          title: "Skip It - Better Alternatives",
          message: "Based on your inputs, Express Pass doesn't offer strong value. Strategic planning can achieve similar results for free.",
          tips: [
            "Arrive at park opening - first hour is golden",
            "Use single rider lines to cut waits 50%+",
            "Ride popular attractions during parades/shows",
            "Consider visiting on a lower-crowd day instead"
          ]
        };
      }
    },

    formatTime(minutes) {
      if (minutes < 60) return `${minutes}m`;
      const hours = Math.floor(minutes / 60);
      const mins = minutes % 60;
      return mins > 0 ? `${hours}h ${mins}m` : `${hours}h`;
    }
  };
}
</script>
